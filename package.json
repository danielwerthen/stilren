{
  "name": "stilren",
  "description": "Expressive and highly extendable styling api",
  "version": "0.1.0-alpha",
  "main": "dist/index.js",
  "repository": "git@github.com:danielwerthen/stilren.git",
  "author": "<danielwerthen@gmail.com>",
  "license": "MIT",
  "private": false,
  "files": [
    "dist/**/*",
    "react.js",
    "react.d.js"
  ],
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/preset-typescript": "^7.7.7",
    "@types/node": "^13.1.4",
    "@types/react": "^16.9.17",
    "babel-jest": "^24.9.0",
    "jest": "*",
    "prettier": "*",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "rimraf": "*",
    "styletron-engine-atomic": "^1.4.4",
    "tsc": "^1.20150623.0",
    "typescript": "^3.7.4"
  },
  "scripts": {
    "test": "jest",
    "prepublish": "npm run build",
    "postinstall": "node dist/postinstall.js",
    "build": "rimraf dist && babel --extensions \".ts,.tsx\" src -d dist && rimraf dist/**/*.spec.js && rimraf dist/*.spec.js && tsc"
  },
  "peerDependencies": {
    "react": "*"
  },
  "stilren": {
    "mediaPrefixes": {
      "small": "(max-width: 768px)",
      "medium": "(max-width: 1024px) and (min-width: 769px)",
      "large": "(min-width: 1025px)"
    },
    "pseudoSuffixes": "focus, hover"
  },
  "dependencies": {
    "csstype": "^2.6.8",
    "humps": "^2.0.1",
    "mdn-data": "^2.0.6"
  }
}
