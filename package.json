{
  "name": "stilren",
  "description": "Expressive and highly extendable styling api",
  "repository": "git@github.com:danielwerthen/stilren.git",
  "author": "<danielwerthen@gmail.com>",
  "private": false,
  "version": "0.4.0-beta2",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "jsx-dev-runtime.d.ts",
    "jsx-dev-runtime.js",
    "jsx-runtime.d.ts",
    "jsx-runtime.js"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "module": "dist/tsdx.esm.js",
  "size-limit": [
    {
      "path": "dist/tsdx.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/tsdx.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@size-limit/preset-small-lib": "^4.9.2",
    "@storybook/addon-essentials": "^6.1.16",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.1.16",
    "@storybook/addons": "^6.1.16",
    "@storybook/react": "^6.1.16",
    "@types/base-react": "npm:@types/react@17.0.1",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "babel-loader": "^8.2.2",
    "eslint": "7.17.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.3.8",
    "prettier": "^2.2.1",
    "react-dom": "^17.0.1",
    "react-is": "^17.0.1",
    "size-limit": "^4.9.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@types/styletron-standard": "^2.0.1",
    "base-react": "npm:react@17.0.1",
    "csstype": "^3.0.6",
    "styletron-standard": "^3.0.4"
  },
  "resolutions": {
    "eslint": "7.17.0",
    "eslint-config-react-app": "6.0.0",
    "typescript": "4.1.3"
  }
}
